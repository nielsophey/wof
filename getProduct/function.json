{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "product",
      "databaseName": "woficecream",
      "collection": "products",
      "connectionStringSetting": "CosmosDBConnectionString",
      "direction": "in",
      "Id": "{Query.id}"
    }
  ]
}
